datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Doctor {
  averageRating Decimal?
  createdAt     DateTime @default(now())
  id            String   @id @default(cuid())
  name          String
  speciality    String
  totalRatings  Int?
  updatedAt     DateTime @updatedAt
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}
